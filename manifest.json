{
    "manifest_version": 2,

    "name": "Enhanced Community Market",
    "short_name": "Enhanced Community Market",
    "description": "Load your inventory and sell items at the lowest price",
    "version": "0.0.1",
    "icons": {
        "128": "images/steam_512.png"
    },
    "options_page": "inc/main.html",
    "permissions": [
        "storage",
        "cookies",
        "notifications",
        "http://*/*"

    ],
    "web_accessible_resources": [
        "js/bootstrap.min.js",
        "js/jquery.cookie.js",
        "js/dashboard.js",
        "js/settings.js",
        "js/jquery-2.1.0.min.js"
    ],
    "browser_action": {
        "default_icon": {
            "19": "images/19.png",
            "38": "images/38.png"
        },
        "default_title": "Main",
        "default_popup": "inc/main.html"
    }
}
